<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>wyr.servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>MemberServlet</servlet-name>
    <servlet-class>wyr.servlet.MemberServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DiscountServlet</servlet-name>
    <servlet-class>wyr.servlet.DiscountServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>GoodsServlet</servlet-name>
    <servlet-class>wyr.servlet.GoodsServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>BillServlet</servlet-name>
    <servlet-class>wyr.servlet.BillServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>DetailBillServlet</servlet-name>
    <servlet-class>wyr.servlet.DetailBillServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PersonMemberServlet</servlet-name>
    <servlet-class>wyr.servlet.PersonMemberServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PersonDiscountServlet</servlet-name>
    <servlet-class>wyr.servlet.PersonDiscountServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>PersonDetailCountServlet</servlet-name>
    <servlet-class>wyr.servlet.PersonDetailCountServlet</servlet-class>
  </servlet>







  


  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>MemberServlet</servlet-name>
    <url-pattern>/MemberServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DiscountServlet</servlet-name>
    <url-pattern>/DiscountServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GoodsServlet</servlet-name>
    <url-pattern>/GoodsServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>BillServlet</servlet-name>
    <url-pattern>/BillServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DetailBillServlet</servlet-name>
    <url-pattern>/DetailBillServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PersonMemberServlet</servlet-name>
    <url-pattern>/PersonMemberServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PersonDiscountServlet</servlet-name>
    <url-pattern>/PersonDiscountServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>PersonDetailCountServlet</servlet-name>
    <url-pattern>/PersonDetailCountServlet</url-pattern>
  </servlet-mapping>

    <welcome-file-list>
        <welcome-file>/jsp/login.jsp</welcome-file>
    </welcome-file-list>

</web-app>
